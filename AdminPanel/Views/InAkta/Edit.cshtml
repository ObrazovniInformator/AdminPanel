<script type="text/javascript">
    $(document).ready(function () {
        $("#IdPodvrsta").change(function () {

            var url = '@Url.Content("~/")' + "InAkta/GetRubrika";
            var ddlsource = "#IdPodvrsta";
            $.getJSON(url, { IdPodvrsta: $(ddlsource).val() }, function (data) {
                var items = '';
                $("#IdRubrika").empty();
                $.each(data, function (i, rubrika) {
                    items += "<option value='" + rubrika.value + "'>" + rubrika.text + "</option>";
                });
                $("#IdRubrika").html(items);
            });
        });

        $("#IdRubrika").change(function () {
            var url = '@Url.Content("~/")' + "InAkta/GetPodrubrika";
            var ddlsource = "#IdRubrika";
            $.getJSON(url, { IdRubrika: $(ddlsource).val() }, function (data) {
                var items = '';
                $("#IdPodrubrika").empty();
                $.each(data, function (i, podrubrika) {
                    items += "<option value='" + podrubrika.value + "'>" + podrubrika.text + "</option>";
                });
                $("#IdPodrubrika").html(items);
            });
        });

        $("#IdPodrubrika").change(function () {
            var url = '@Url.Content("~/")' + "InAkta/GetPodpodrubrika";
            var ddlsource = "#IdPodrubrika";
            $.getJSON(url, { IdPodrubrika: $(ddlsource).val() }, function (data) {
                var items = '';
                $("#IdPodpodrubrika").empty();
                $.each(data, function (i, podpodrubrika) {
                    items += "<option value='" + podpodrubrika.value + "'>" + podpodrubrika.text + "</option>";
