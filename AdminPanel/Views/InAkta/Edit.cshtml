            $.getJSON(url, { IdPodpodrubrika: $(ddlsource).val() }, function (data) {
                var items = '';
                $("#IdPodpodpodrubrika").empty();
                $.each(data, function (i, podpodpodrubrika) {
                    items += "<option value='" + podpodpodrubrika.value + "'>" + podpodpodrubrika.text + "</option>";
